{
    "name": "dreibein/contao-activity-bundle",
    "type": "contao-bundle",
    "description": "Dieses Bundle erfasst die Dauer der Arbeiten aller Benutzer im Backend von Contao. Diese werden addiert und im Dasboard ausgegeben. So erhält man eine schnelle Übersicht, welche Nutzer wie lange aktiv sind.",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Werbeagentur Dreibein GmbH",
            "homepage": "https://www.agentur-dreibein.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.9",
        "doctrine/doctrine-bundle": "^2.2.4",
        "doctrine/orm": "^2.8"
    },
    "require-dev": {
        "roave/security-advisories": "*@dev",
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "contao-manager-plugin": "Contao\\ActivityBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Contao\\ActivityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ActivityBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/dreibein/contao-activity-bundle/issues",
        "source": "https://github.com/dreibein/contao-activity-bundle"
    }
}
