{
    "name": "dreibein/contao-activity-bundle",
    "type": "contao-bundle",
    "description": "Activity Bundle",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Werbeagentur Dreibein GmbH",
            "homepage": "https://www.agentur-dreibein.de"
        }
    ],
    "require": {
        "php": ">=7.4",
        "contao/core-bundle": "^4.9.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "extra": {
        "contao-manager-plugin": "Contao\\ActivityBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Contao\\ActivityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ActivityBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/dreibein/activity-bundle/issues",
        "source": "https://github.com/dreibein/activity-bundle"
    }
}
